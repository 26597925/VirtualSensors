<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.com.sensor.android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="10" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name=".TYPE_LIGHT"
            android:label="@string/app_name" />
          <activity
            android:name=".TYPE_ORIENTATION"
            android:label="@string/app_name" />
           <activity
            android:name=".TYPE_PROXIMITY"
            android:label="@string/app_name" />
           <activity
            android:name=".TYPE_MAGNETIC_FIELD"
            android:label="@string/app_name" />
           <activity
            android:name=".TYPE_ACCELEROMETER"
            android:label="@string/app_name" />
            <activity
            android:name=".TYPE_GYROSCOPE"
            android:label="@string/app_name" />
             <activity
            android:name=".TYPE_TEMPERATURE"
            android:label="@string/app_name" />
              <activity
            android:name=".TYPE_LINEAR_ACCELERATION"
            android:label="@string/app_name" />
    </application>

</manifest>